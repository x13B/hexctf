// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userID      Int    @id @default(autoincrement())
  username    String @unique
  password    String
  email       String @unique
}

model Admin {
  
}